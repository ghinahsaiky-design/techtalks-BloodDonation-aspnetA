@model IEnumerable<BloodDonation.Models.DonorProfile>
@{
    Layout = null;
    ViewData["Title"] = "Admin Donor Management";
}

<!DOCTYPE html>
<html class="light" lang="en"><head>
<meta charset="utf-8"/>
<meta content="width=device-width, initial-scale=1.0" name="viewport"/>
<title>BloodConnect Admin Donor CRUD Management</title>
<script src="https://cdn.tailwindcss.com?plugins=forms,container-queries"></script>
<link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;700;900&amp;display=swap" rel="stylesheet"/>
<link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:wght,FILL@100..700,0..1&amp;display=swap" rel="stylesheet"/>
<style>
        .material-symbols-outlined {
            font-variation-settings:
                'FILL' 0,
                'wght' 400,
                'GRAD' 0,
                'opsz' 24
        }.modal-scroll::-webkit-scrollbar {
            width: 8px;
        }
        .modal-scroll::-webkit-scrollbar-track {
            background: #f1f1f1;
        }
        .modal-scroll::-webkit-scrollbar-thumb {
            background: #d1d5db;
            border-radius: 4px;
        }
        .modal-scroll::-webkit-scrollbar-thumb:hover {
            background: #9ca3af;
        }
    </style>
<script id="tailwind-config">
        tailwind.config = {
            darkMode: "class",
            theme: {
                extend: {
                    colors: {
                        "primary": "#d70427",
                        "background-light": "#f8f5f6",
                        "background-dark": "#230f12",
                        "blue-700": "#1E40AF",
                        "gray-100": "#F3F4F6",
                        "gray-800": "#1F2937",
                        "gray-500": "#6B7280",
                    },
                    fontFamily: {
                        "display": ["Inter", "sans-serif"]
                    },
                    borderRadius: {
                        "DEFAULT": "0.25rem",
                        "lg": "0.5rem",
                        "xl": "0.75rem",
                        "full": "9999px"
                    },
                },
            },
        }
    </script>
</head>
<body class="font-display bg-background-light dark:bg-background-dark text-gray-800 dark:text-gray-100 overflow-hidden">
<div class="relative flex min-h-screen w-full flex-row group/design-root">
<aside class="flex h-screen min-h-screen w-64 flex-col bg-white dark:bg-background-dark dark:border-r dark:border-white/10 shadow-sm z-20">
<div class="flex h-full flex-col justify-between p-4">
<div class="flex flex-col gap-4">
<div class="flex items-center gap-3 px-3 py-2">
<img src="~/images/BloodDonationLogo.png" alt="BloodConnect logo" class="size-10 rounded-full object-cover"/>
<div class="flex flex-col">
<h1 class="text-gray-800 dark:text-white text-base font-bold leading-normal">BloodConnect</h1>
<p class="text-gray-500 dark:text-gray-400 text-sm font-normal leading-normal">Admin Panel</p>
</div>
</div>
<nav class="flex flex-col gap-2 mt-4">
<a class="flex items-center gap-3 px-3 py-2 text-gray-800 dark:text-gray-300 rounded-lg hover:bg-background-light dark:hover:bg-white/5 transition-colors" href="@Url.Action("Index", "Admin")">
<span class="material-symbols-outlined text-blue-700 dark:text-primary/80">dashboard</span>
<p class="text-sm font-medium leading-normal">Dashboard</p>
</a>
<a class="flex items-center gap-3 px-3 py-2 rounded-lg bg-primary/10 text-primary dark:bg-primary/20 dark:text-primary transition-colors" href="@Url.Action("DonorManagement", "Admin")">
<span class="material-symbols-outlined !fill-1">group</span>
<p class="text-sm font-bold leading-normal">Donors</p>
</a>
<a class="flex items-center gap-3 px-3 py-2 text-gray-800 dark:text-gray-300 rounded-lg hover:bg-background-light dark:hover:bg-white/5 transition-colors" href="@Url.Action("RequestManagement", "Admin")">
<span class="material-symbols-outlined text-blue-700 dark:text-primary/80">inbox</span>
<p class="text-sm font-medium leading-normal">Requests</p>
</a>
<a class="flex items-center gap-3 px-3 py-2 text-gray-800 dark:text-gray-300 rounded-lg hover:bg-background-light dark:hover:bg-white/5 transition-colors" href="@Url.Action("Settings", "Admin")">
<span class="material-symbols-outlined text-blue-700 dark:text-primary/80">settings</span>
<p class="text-sm font-medium leading-normal">Settings</p>
</a>
</nav>
</div>
<div class="px-3 py-2">
<form asp-action="Logout" asp-controller="Auth" method="post" class="inline">
@Html.AntiForgeryToken()
<button type="submit" class="flex items-center gap-3 text-red-600 hover:text-red-700 transition-colors w-full">
<span class="material-symbols-outlined">logout</span>
<p class="text-sm font-medium leading-normal">Sign Out</p>
</button>
</form>
</div>
</div>
</aside>
<div class="flex-1 flex flex-col h-screen overflow-hidden relative">
<header class="flex items-center justify-between whitespace-nowrap border-b border-solid border-gray-200 dark:border-white/10 bg-white dark:bg-background-dark px-8 py-3 sticky top-0 z-10 shadow-sm">
<div class="flex items-center gap-4 text-gray-800 dark:text-white">
<h2 class="text-lg font-bold leading-tight tracking-[-0.015em]">Donor Management</h2>
</div>
<div class="flex flex-1 justify-end gap-4 items-center">
<div class="relative">
<button class="flex items-center justify-center rounded-full size-10 hover:bg-gray-100 dark:hover:bg-white/5 transition-colors text-gray-500">
<span class="material-symbols-outlined">notifications</span>
<span class="absolute top-2 right-2 size-2 bg-primary rounded-full border-2 border-white dark:border-background-dark"></span>
</button>
</div>
<div class="h-8 w-px bg-gray-200 dark:bg-white/10 mx-2"></div>
<div class="flex items-center gap-3">
<div class="text-right hidden md:block">
<p class="text-sm font-semibold text-gray-800 dark:text-white">@ViewBag.AdminName</p>
<p class="text-xs text-gray-500">Admin</p>
</div>
<img src="~/images/BloodDonationLogo.png" alt="Admin user avatar" class="size-10 rounded-full object-cover border-2 border-white dark:border-white/10 shadow-sm"/>
</div>
</div>
</header>
<main class="flex-1 overflow-y-auto p-6 md:p-8 bg-gray-50 dark:bg-[#1a0b0d]">
<div class="max-w-7xl mx-auto space-y-6">
<div class="flex flex-wrap justify-between items-center gap-4">
<div>
<h1 class="text-2xl font-bold text-gray-900 dark:text-white tracking-tight">Donor Records</h1>
<p class="text-sm text-gray-500 mt-1">Manage and track all registered blood donors.</p>
</div>
<a href="@Url.Action("AddDonor", "Admin")" class="flex items-center gap-2 cursor-pointer justify-center rounded-lg h-10 px-5 bg-primary text-white text-sm font-semibold shadow-md shadow-primary/20 hover:bg-red-700 transition-all active:scale-95">
<span class="material-symbols-outlined text-[20px]">add</span>
<span>Add New Donor</span>
</a>
</div>
<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-4">
<div class="flex flex-col gap-1 rounded-xl p-5 border border-gray-100 dark:border-white/5 bg-white dark:bg-background-dark shadow-sm">
<div class="flex items-center gap-3 mb-1">
<div class="p-2 bg-blue-50 text-blue-600 rounded-lg dark:bg-blue-900/20 dark:text-blue-400">
<span class="material-symbols-outlined text-[20px]">group</span>
</div>
<p class="text-gray-500 dark:text-gray-400 text-sm font-medium">Total Donors</p>
</div>
<p class="text-gray-900 dark:text-white text-2xl font-bold pl-1">@ViewBag.TotalDonors.ToString("N0")</p>
<div class="pl-1 flex items-center gap-1 text-xs text-green-600 font-medium">
<span class="material-symbols-outlined text-[14px]">trending_up</span>
<span>Total registered</span>
</div>
</div>
<div class="flex flex-col gap-1 rounded-xl p-5 border border-gray-100 dark:border-white/5 bg-white dark:bg-background-dark shadow-sm">
<div class="flex items-center gap-3 mb-1">
<div class="p-2 bg-green-50 text-green-600 rounded-lg dark:bg-green-900/20 dark:text-green-400">
<span class="material-symbols-outlined text-[20px]">check_circle</span>
</div>
<p class="text-gray-500 dark:text-gray-400 text-sm font-medium">Available</p>
</div>
<p class="text-gray-900 dark:text-white text-2xl font-bold pl-1">@ViewBag.AvailableDonors.ToString("N0")</p>
<div class="pl-1 text-xs text-gray-400">
                                Ready to donate
                            </div>
</div>
<div class="flex flex-col gap-1 rounded-xl p-5 border border-gray-100 dark:border-white/5 bg-white dark:bg-background-dark shadow-sm">
<div class="flex items-center gap-3 mb-1">
<div class="p-2 bg-red-50 text-red-600 rounded-lg dark:bg-red-900/20 dark:text-red-400">
<span class="material-symbols-outlined text-[20px]">bloodtype</span>
</div>
<p class="text-gray-500 dark:text-gray-400 text-sm font-medium">Rare Blood Type</p>
</div>
<p class="text-gray-900 dark:text-white text-2xl font-bold pl-1">@ViewBag.RareBloodTypeCount</p>
<div class="pl-1 text-xs text-red-500 font-medium">
                                AB-, O-, B-, A-
                            </div>
</div>
<div class="flex flex-col gap-1 rounded-xl p-5 border border-gray-100 dark:border-white/5 bg-white dark:bg-background-dark shadow-sm">
<div class="flex items-center gap-3 mb-1">
<div class="p-2 bg-purple-50 text-purple-600 rounded-lg dark:bg-purple-900/20 dark:text-purple-400">
<span class="material-symbols-outlined text-[20px]">location_on</span>
</div>
<p class="text-gray-500 dark:text-gray-400 text-sm font-medium">Top Region</p>
</div>
<p class="text-gray-900 dark:text-white text-2xl font-bold pl-1">@ViewBag.TopRegion</p>
<div class="pl-1 text-xs text-gray-400">
                                @ViewBag.TopRegionCount Active Donors
                            </div>
</div>
</div>
<div class="bg-white dark:bg-background-dark rounded-xl shadow-sm border border-gray-200 dark:border-white/5 overflow-hidden">
<div class="flex flex-col gap-4 p-5 border-b border-gray-100 dark:border-white/5">
<form method="get" action="@Url.Action("DonorManagement", "Admin")" class="flex flex-col gap-4 w-full">
<div class="relative w-full sm:max-w-xs">
<div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
<span class="material-symbols-outlined text-gray-400 text-[20px]">search</span>
</div>
<input name="searchTerm" value="@ViewBag.SearchTerm" class="w-full pl-10 pr-4 py-2.5 rounded-lg border border-gray-200 dark:border-white/10 bg-gray-50 dark:bg-white/5 text-sm focus:ring-2 focus:ring-primary/20 focus:border-primary placeholder-gray-400 transition-all" placeholder="Search by name, email or ID..." type="text"/>
</div>
<div class="flex w-full sm:w-auto gap-3">
<div class="relative group">
<span class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
<span class="material-symbols-outlined text-gray-400 text-[18px]">filter_list</span>
</span>
<select name="bloodTypeFilter" onchange="this.form.submit()" class="pl-9 pr-8 py-2.5 rounded-lg border border-gray-200 dark:border-white/10 bg-white dark:bg-white/5 focus:ring-2 focus:ring-primary/20 focus:border-primary text-sm appearance-none cursor-pointer w-full sm:w-auto">
<option value="">All Blood Types</option>
@foreach (var bt in ViewBag.BloodTypes as List<BloodDonation.Models.BloodTypes> ?? new List<BloodDonation.Models.BloodTypes>())
{
    <option value="@bt.Type" selected="@(ViewBag.BloodTypeFilter == bt.Type)">@bt.Type</option>
}
</select>
<div class="absolute inset-y-0 right-0 pr-3 flex items-center pointer-events-none">
<span class="material-symbols-outlined text-gray-400 text-[18px]">expand_more</span>
</div>
</div>
<div class="relative group">
<span class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
<span class="material-symbols-outlined text-gray-400 text-[18px]">toggle_on</span>
</span>
<select name="statusFilter" onchange="this.form.submit()" class="pl-9 pr-8 py-2.5 rounded-lg border border-gray-200 dark:border-white/10 bg-white dark:bg-white/5 focus:ring-2 focus:ring-primary/20 focus:border-primary text-sm appearance-none cursor-pointer w-full sm:w-auto">
<option value="">All Status</option>
<option value="Available" selected="@(ViewBag.StatusFilter == "Available")">Available</option>
<option value="Unavailable" selected="@(ViewBag.StatusFilter == "Unavailable")">Unavailable</option>
</select>
<div class="absolute inset-y-0 right-0 pr-3 flex items-center pointer-events-none">
<span class="material-symbols-outlined text-gray-400 text-[18px]">expand_more</span>
</div>
</div>
<button type="submit" class="px-4 py-2.5 rounded-lg bg-primary text-white text-sm font-medium hover:bg-primary/90">Search</button>
</form>
</div>
<div class="overflow-x-auto">
<table class="w-full text-sm text-left border-collapse">
<thead class="bg-gray-50 dark:bg-white/5 text-xs uppercase font-semibold text-gray-500 dark:text-gray-400 border-b border-gray-200 dark:border-white/5">
<tr>
<th class="px-6 py-4 w-20 text-center align-middle" scope="col">ID</th>
<th class="px-6 py-4 align-middle" scope="col">Donor Details</th>
<th class="px-6 py-4 align-middle" scope="col">Blood Type</th>
<th class="px-6 py-4 align-middle" scope="col">Location</th>
<th class="px-6 py-4 align-middle" scope="col">Status</th>
<th class="px-6 py-4 text-center align-middle" scope="col">Last Donation</th>
<th class="px-6 py-4 text-right align-middle" scope="col">Actions</th>
</tr>
</thead>
<tbody class="divide-y divide-gray-100 dark:divide-white/5">
@if (Model == null || !Model.Any())
{
    <tr>
        <td colspan="7" class="px-6 py-8 text-center text-gray-500 align-middle">No donors found</td>
    </tr>
}
else
{
    @foreach (var donor in Model)
    {
        var donorName = donor.User.FirstName + " " + donor.User.LastName;
        var initials = donor.User.FirstName.Substring(0, 1) + donor.User.LastName.Substring(0, 1);
        var isAvailable = donor.IsAvailable && donor.IsHealthyForDonation;
        var daysSince = donor.LastDonationDate.HasValue 
            ? (DateTime.UtcNow - donor.LastDonationDate.Value).Days 
            : (int?)null;
        var lastDonationText = daysSince switch
        {
            null => "Never",
            < 30 => $"{daysSince} days ago",
            < 60 => "1 month ago",
            < 365 => $"{daysSince / 30} months ago",
            _ => $"{daysSince / 365} years ago"
        };
        
        <tr class="hover:bg-gray-50 dark:hover:bg-white/5 transition-colors group">
            <td class="px-6 py-4 font-mono text-gray-400 text-center align-middle">#@donor.DonorId</td>
            <td class="px-6 py-4 align-middle">
                <div class="flex flex-col gap-1">
                    <div class="font-semibold text-gray-900 dark:text-white">@donorName</div>
                    <div class="text-xs text-gray-500">@donor.User.Email</div>
                </div>
            </td>
            <td class="px-6 py-4 align-middle">
                <span class="inline-flex items-center justify-center px-2.5 py-1 rounded-md text-xs font-bold bg-red-50 text-red-700 dark:bg-red-900/30 dark:text-red-400 border border-red-100 dark:border-red-900/50 whitespace-nowrap">
                    @donor.BloodType.Type
                </span>
            </td>
            <td class="px-6 py-4 text-gray-600 dark:text-gray-300 align-middle whitespace-nowrap">@donor.Location.Districts</td>
            <td class="px-6 py-4 align-middle">
                @if (isAvailable)
                {
                    <span class="inline-flex items-center gap-1.5 px-2.5 py-1 rounded-full text-xs font-medium bg-green-50 text-green-700 dark:bg-green-900/20 dark:text-green-400 border border-green-100 dark:border-green-900/30 whitespace-nowrap">
                        <span class="size-1.5 rounded-full bg-green-500 animate-pulse"></span>
                        Available
                    </span>
                }
                else
                {
                    <span class="inline-flex items-center gap-1.5 px-2.5 py-1 rounded-full text-xs font-medium bg-gray-100 text-gray-600 dark:bg-gray-800 dark:text-gray-400 border border-gray-200 dark:border-white/10 whitespace-nowrap">
                        <span class="size-1.5 rounded-full bg-gray-400"></span>
                        Unavailable
                    </span>
                }
            </td>
            <td class="px-6 py-4 text-center text-gray-500 text-xs align-middle whitespace-nowrap">@lastDonationText</td>
            <td class="px-6 py-4 text-right align-middle">
                <div class="flex items-center justify-end gap-2 opacity-0 group-hover:opacity-100 transition-opacity">
                    <a href="@Url.Action("EditDonor", "Admin", new { id = donor.DonorId })" class="p-1.5 rounded-md text-blue-600 hover:bg-blue-50 dark:hover:bg-blue-900/20 transition-colors" title="Edit Donor">
                        <span class="material-symbols-outlined text-[18px]">edit</span>
                    </a>
                    <form asp-action="DeleteDonor" asp-controller="Admin" method="post" class="inline" onsubmit="return confirm('Are you sure you want to delete this donor? This action cannot be undone.');">
                        @Html.AntiForgeryToken()
                        <input type="hidden" name="id" value="@donor.DonorId" />
                        <button type="submit" class="p-1.5 rounded-md text-red-600 hover:bg-red-50 dark:hover:bg-red-900/20 transition-colors" title="Delete Donor">
                            <span class="material-symbols-outlined text-[18px]">delete</span>
                        </button>
                    </form>
                </div>
            </td>
        </tr>
    }
}
</tbody>
</table>
</div>
<div class="flex flex-col sm:flex-row justify-between items-center gap-4 p-4 border-t border-gray-100 dark:border-white/5 bg-gray-50/50 dark:bg-white/5">
<span class="text-sm text-gray-500 dark:text-gray-400">Showing <span class="font-medium text-gray-900 dark:text-white">@((Model?.Count() ?? 0) > 0 ? 1 : 0)</span> to <span class="font-medium text-gray-900 dark:text-white">@(Model?.Count() ?? 0)</span> of <span class="font-medium text-gray-900 dark:text-white">@ViewBag.TotalCount</span> entries</span>
<nav aria-label="Pagination" class="isolate inline-flex -space-x-px rounded-md shadow-sm">
<a class="relative inline-flex items-center rounded-l-md px-2 py-2 text-gray-400 ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-20 focus:outline-offset-0 dark:ring-white/10 dark:hover:bg-white/5" href="#">
<span class="sr-only">Previous</span>
<span class="material-symbols-outlined text-[20px]">chevron_left</span>
</a>
<a aria-current="page" class="relative z-10 inline-flex items-center bg-primary px-4 py-2 text-sm font-semibold text-white focus:z-20 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600" href="#">1</a>
<a class="relative inline-flex items-center px-4 py-2 text-sm font-semibold text-gray-900 ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-20 focus:outline-offset-0 dark:text-gray-300 dark:ring-white/10 dark:hover:bg-white/5" href="#">2</a>
<a class="relative hidden items-center px-4 py-2 text-sm font-semibold text-gray-900 ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-20 focus:outline-offset-0 md:inline-flex dark:text-gray-300 dark:ring-white/10 dark:hover:bg-white/5" href="#">3</a>
<span class="relative inline-flex items-center px-4 py-2 text-sm font-semibold text-gray-700 ring-1 ring-inset ring-gray-300 focus:outline-offset-0 dark:text-gray-500 dark:ring-white/10">...</span>
<a class="relative hidden items-center px-4 py-2 text-sm font-semibold text-gray-900 ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-20 focus:outline-offset-0 md:inline-flex dark:text-gray-300 dark:ring-white/10 dark:hover:bg-white/5" href="#">8</a>
<a class="relative inline-flex items-center rounded-r-md px-2 py-2 text-gray-400 ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-20 focus:outline-offset-0 dark:ring-white/10 dark:hover:bg-white/5" href="#">
<span class="sr-only">Next</span>
<span class="material-symbols-outlined text-[20px]">chevron_right</span>
</a>
</nav>
</div>
</div>
</div>
</main>
</div>
<div class="fixed inset-0 z-50 flex items-center justify-center p-4 sm:p-6" style="display: none;"> 
<div class="absolute inset-0 bg-gray-900/60 backdrop-blur-sm transition-opacity"></div>
<div class="relative w-full max-w-2xl transform overflow-hidden rounded-xl bg-white dark:bg-background-dark p-6 text-left shadow-2xl transition-all border border-gray-100 dark:border-white/10">
<div class="flex items-center justify-between mb-5">
<h3 class="text-xl font-bold leading-6 text-gray-900 dark:text-white">Add New Donor</h3>
<button class="text-gray-400 hover:text-gray-500 transition-colors">
<span class="material-symbols-outlined">close</span>
</button>
</div>
<form class="space-y-4">
<div class="grid grid-cols-1 md:grid-cols-2 gap-4">
<div class="space-y-1">
<label class="text-sm font-medium text-gray-700 dark:text-gray-300">Full Name</label>
<input class="w-full rounded-lg border-gray-300 dark:border-white/20 dark:bg-white/5 shadow-sm focus:border-primary focus:ring-primary sm:text-sm" placeholder="e.g. John Doe" type="text"/>
</div>
<div class="space-y-1">
<label class="text-sm font-medium text-gray-700 dark:text-gray-300">Email Address</label>
<input class="w-full rounded-lg border-gray-300 dark:border-white/20 dark:bg-white/5 shadow-sm focus:border-primary focus:ring-primary sm:text-sm" placeholder="john@example.com" type="email"/>
</div>
<div class="space-y-1">
<label class="text-sm font-medium text-gray-700 dark:text-gray-300">Blood Type</label>
<select class="w-full rounded-lg border-gray-300 dark:border-white/20 dark:bg-white/5 shadow-sm focus:border-primary focus:ring-primary sm:text-sm">
<option>Select type...</option>
<option>A+</option>
<option>O-</option>
</select>
</div>
<div class="space-y-1">
<label class="text-sm font-medium text-gray-700 dark:text-gray-300">Phone Number</label>
<input class="w-full rounded-lg border-gray-300 dark:border-white/20 dark:bg-white/5 shadow-sm focus:border-primary focus:ring-primary sm:text-sm" placeholder="+1 (555) 000-0000" type="tel"/>
</div>
</div>
<div class="space-y-1">
<label class="text-sm font-medium text-gray-700 dark:text-gray-300">Location / Address</label>
<input class="w-full rounded-lg border-gray-300 dark:border-white/20 dark:bg-white/5 shadow-sm focus:border-primary focus:ring-primary sm:text-sm" placeholder="e.g. 123 Main St, New York, NY" type="text"/>
</div>
<div class="flex items-center gap-2 pt-2">
<input class="h-4 w-4 rounded border-gray-300 text-primary focus:ring-primary" id="available" type="checkbox"/>
<label class="text-sm text-gray-700 dark:text-gray-300" for="available">Donor is currently available for donation</label>
</div>
<div class="mt-6 flex justify-end gap-3 border-t border-gray-100 dark:border-white/10 pt-4">
<button class="rounded-lg border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 shadow-sm hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-primary focus:ring-offset-2" type="button">Cancel</button>
<button class="rounded-lg border border-transparent bg-primary px-4 py-2 text-sm font-medium text-white shadow-sm hover:bg-red-700 focus:outline-none focus:ring-2 focus:ring-primary focus:ring-offset-2" type="submit">Save Donor</button>
</div>
</form>
</div>
